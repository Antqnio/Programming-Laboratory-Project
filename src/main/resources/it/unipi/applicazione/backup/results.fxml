<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox fx:id="myVBox" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.unipi.applicazione.ResultsController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <Button fx:id="getBackButton" mnemonicParsing="false" onAction="#getBack" text="Indietro" />
      <Button fx:id="switchLanguageButton" mnemonicParsing="false" onAction="#switchLanguage" text="Button" />
      <Label fx:id="searchResultsLabel" text="Risultati della ricerca" />
      <Label fx:id="gameLabel" text="Giochi" />
      <TableView fx:id="gameTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showGameDescriptionMI" mnemonicParsing="false" onAction="#showGameDescription" text="Mostra Descrizione" />
                  <MenuItem fx:id="addGameToFavoritesMI" mnemonicParsing="false" onAction="#addGameToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="characterLabel" text="Personaggi" />
      <TableView fx:id="characterTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showCharacterDescriptionMI" mnemonicParsing="false" onAction="#showCharacterDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addCharacterToFavoritesMI" mnemonicParsing="false" onAction="#addCharacterToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="monsterLabel" text="Mostri" />
      <TableView fx:id="monsterTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showMonsterDescriptionMI" mnemonicParsing="false" onAction="#showMonsterDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addMonsterToFavoritesMI" mnemonicParsing="false" onAction="#addMonsterToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="placeLabel" text="Posti" />
      <TableView fx:id="placeTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showPlaceDescriptionMI" mnemonicParsing="false" onAction="#showPlaceDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addPlaceToFavoritesMI" mnemonicParsing="false" onAction="#addPlaceToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="itemLabel" text="Oggetti" />
      <TableView fx:id="itemTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showItemDescriptionMI" mnemonicParsing="false" onAction="#showItemDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addItemToFavoritesMI" mnemonicParsing="false" onAction="#addItemToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="dungeonLabel" text="Dungeon" />
      <TableView fx:id="dungeonTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showDungeonDescriptionMI" mnemonicParsing="false" onAction="#showDungeonDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addDungeonToFavoritesMI" mnemonicParsing="false" onAction="#addDungeonToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
      <Label fx:id="bossLabel" text="Boss" />
      <TableView fx:id="bossTable" prefHeight="200.0" prefWidth="200.0">
         <contextMenu>
            <ContextMenu>
              <items>
                  <MenuItem fx:id="showBossDescriptionMI" mnemonicParsing="false" onAction="#showBossDescription" text="Unspecified Action" />
                  <MenuItem fx:id="addBossToFavoritesMI" mnemonicParsing="false" onAction="#addBossToFavorites" text="Unspecified Action" />
              </items>
            </ContextMenu>
         </contextMenu></TableView>
   </children>
</VBox>
